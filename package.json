{
  "name": "excel-copilot-addin",
  "version": "3.0.11",
  "type": "module",
  "description": "Excel 智能助手 Add-in - AI-powered Excel assistant with natural language processing",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane.git"
  },
  "author": "Excel 智能助手 Team",
  "license": "MIT",
  "keywords": [
    "excel",
    "office-addin",
    "ai",
    "copilot",
    "deepseek",
    "react",
    "typescript"
  ],
  "config": {
    "app_to_debug": "excel",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000,
    "ai_backend_port": 3001,
    "mock_backend_port": 3002
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:analyze": "ANALYZE=true webpack --mode production",
    "dev-server": "webpack serve --mode development",
    "dev": "npm run start:all",
    "dev:full": "concurrently \"npm run dev-server\" \"npm run ai-backend\" \"npm run mock-backend\"",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint:strict": "eslint src/**/*.{ts,tsx} --max-warnings=0",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,css,html}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,css,html}\"",
    "format": "npm run prettier:fix && npm run lint:fix",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "start": "office-addin-debugging start manifest.xml",
    "start:dev": "concurrently \"npm run dev-server\" \"npm run ai-backend\" \"npm run start\"",
    "ai-backend": "node ai-backend.cjs",
    "ai-backend:dev": "nodemon ai-backend.cjs",
    "mock-backend": "node mock-backend.cjs",
    "mock-backend:dev": "nodemon mock-backend.cjs",
    "start:all": "concurrently \"npm run dev-server\" \"npm run ai-backend\"",
    "stop": "office-addin-debugging stop manifest.xml",
    "stop:all": "npm run stop && taskkill /F /IM node.exe 2>nul || true",
    "validate": "office-addin-manifest validate manifest.xml",
    "validate:strict": "office-addin-manifest validate manifest.xml --strict",
    "watch": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:agent": "node tests/agent/test-runner.cjs --ci --report=json",
    "test:agent:blocking": "node tests/agent/test-runner.cjs --ci --blocking-only --report=json",
    "test:agent:verbose": "node tests/agent/test-runner.cjs --verbose",
    "test:agent:report": "node tests/agent/test-runner.cjs --report=markdown",
    "test:agent:stub": "node tests/agent/test-runner.cjs --ci --llm=stub --report=json",
    "test:agent:nightly": "node tests/agent/test-runner.cjs --ci --llm=real --save-trace --report=json",
    "test:agent:pr": "node tests/agent/test-runner.cjs --ci --blocking-only --llm=stub --report=json",
    "test:all": "npm run test && npm run test:agent",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rimraf dist",
    "clean:all": "npm run clean && rimraf node_modules",
    "install:clean": "npm run clean:all && npm install",
    "security-check": "npm audit",
    "security-check:fix": "npm audit fix",
    "outdated": "npm outdated",
    "update": "npm update",
    "update:all": "npm update --save",
    "version": "echo Excel 智能助手 Add-in v$npm_package_version",
    "postinstall": "echo 'Excel 智能助手 Add-in dependencies installed successfully!'"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.72.9",
    "axios": "^1.13.2",
    "body-parser": "^2.2.1",
    "compression": "^1.8.1",
    "core-js": "^3.47.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "openai": "^6.15.0",
    "rate-limiter-flexible": "^5.0.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "regenerator-runtime": "^0.14.1",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.3",
    "@types/office-js": "^1.0.565",
    "@types/office-runtime": "^1.0.35",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-office-addins": "^4.0.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.11",
    "office-addin-cli": "^2.0.6",
    "office-addin-debugging": "^6.0.6",
    "office-addin-dev-certs": "^2.0.6",
    "office-addin-lint": "^3.0.6",
    "office-addin-manifest": "^2.1.2",
    "office-addin-prettier-config": "^2.0.1",
    "os-browserify": "^0.3.0",
    "prettier": "^3.7.4",
    "process": "^0.11.10",
    "rimraf": "^6.1.2",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.ts",
      "!src/setupTests.ts"
    ]
  }
}
